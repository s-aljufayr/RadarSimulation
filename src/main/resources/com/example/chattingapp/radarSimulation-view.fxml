<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<VBox alignment="CENTER" prefHeight="940.0" prefWidth="1433.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.chattingapp.Infantry.RadarSimulationController">

    <children>
        <GridPane id="trackIdField" fx:id="AnimationPan" prefHeight="937.0" prefWidth="1021.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="1067.0" minWidth="10.0" prefWidth="521.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="901.0" minWidth="10.0" prefWidth="862.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="882.0" minHeight="10.0" prefHeight="860.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="276.0" minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
         <children>
                <TabPane prefHeight="711.0" prefWidth="476.0" tabClosingPolicy="UNAVAILABLE">
                    <tabs>
                        <Tab id="connectionProperties" fx:id="connectionProperties" text="Connection Properties">
                            <content>
                                <fx:include source="connectionProperties.fxml" />
                            </content>
                        </Tab>
                        <Tab id="trackControl" fx:id="trackControl" text="Track Control">
                            <content>
                                <fx:include source="trackControl.fxml" />
                            </content>
                        </Tab>
                    </tabs>
                </TabPane>
                <Pane prefHeight="162.0" prefWidth="448.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <children>
                  <Button fx:id="stopSending" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#stopSendingButton" prefHeight="25.0" prefWidth="105.0" text="Button" />
               </children>
                </Pane>
            <Pane prefHeight="793.0" prefWidth="862.0" GridPane.columnIndex="1">
               <children>
                  <WebView id="mapWebView" fx:id="mapWebView" layoutX="9.0" layoutY="13.0" prefHeight="632.0" prefWidth="850.0" />
               </children>
            </Pane>
         </children>
        </GridPane>
    </children>
    <padding>
        <Insets left="10.0" />
    </padding>
</VBox>
